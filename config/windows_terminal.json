{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "alwaysShowTabs": true,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
    "initialCols": 120,
    "initialRows": 40,
    "keybindings": 
    [
        {
            "command": "commandPalette",
            "keys": 
            [
                "ctrl+shift+p"
            ]
        },
        {
            "command": "newTab",
            "keys": 
            [
                "ctrl+t"
            ]
        },
        {
            "command": "closeTab",
            "keys": 
            [
                "ctrl+w"
            ]
        },
        {
            "command": "closePane",
            "keys": 
            [
                "ctrl+shift+d"
            ]
        },
        {
            "command": "copy",
            "keys": 
            [
                "ctrl+c"
            ]
        },
        {
            "command": "paste",
            "keys": 
            [
                "ctrl+v"
            ]
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "horizontal"
            },
            "keys": "ctrl+shift+-"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "vertical"
            },
            "keys": "ctrl+shift+<"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": 
            [
                "ctrl+down"
            ]
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": 
            [
                "ctrl+left"
            ]
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": 
            [
                "ctrl+right"
            ]
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": 
            [
                "ctrl+up"
            ]
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": 
            [
                "ctrl+shift+down"
            ]
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": 
            [
                "ctrl+shift+left"
            ]
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": 
            [
                "ctrl+shift+right"
            ]
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": 
            [
                "ctrl+shift+up"
            ]
        }
    ],
    "profiles": 
